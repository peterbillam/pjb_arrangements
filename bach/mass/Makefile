MUSCRIPT = /home/pbin/muscript

all : sopranini.a4 soprano.a4 alto.a4 tenor.a4 bass.a4

sopranini.a4 : sopranini ${MUSCRIPT}
	${MUSCRIPT} sopranini > $@
soprano.a4 : soprano ${MUSCRIPT}
	${MUSCRIPT} soprano > $@
alto.a4 : alto ${MUSCRIPT}
	${MUSCRIPT} alto > $@
tenor.a4 : tenor ${MUSCRIPT}
	${MUSCRIPT} tenor > $@
bass.a4 : bass ${MUSCRIPT}
	${MUSCRIPT} bass > $@
