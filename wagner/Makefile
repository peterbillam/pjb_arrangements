${D}/c43.mp3: lso_collingwood/audio_43.wav
	sox lso_collingwood/audio_43.wav c43.wav trim 1.2
	${NORMALISE} c43.wav
	${LAME} -h c43.wav $@
	rm c43.wav
	${MPLAYER} $@
${D}/c46.jpg: c46
	bash src2jpg c46 $@
${D}/c46.mp3: lso_collingwood/audio_46.wav
	sox lso_collingwood/audio_46.wav c46.wav trim 1.2
	${NORMALISE} c46.wav
	${LAME} -h c46.wav $@
	rm c46.wav
	${MPLAYER} $@
${D}/c57.jpg: c57
	bash src2jpg c57 $@
${D}/c57.mp3: lso_collingwood/audio_57.wav
	sox lso_collingwood/audio_57.wav c57.wav trim 1.5
	${NORMALISE} c57.wav
	${LAME} -h c57.wav $@
	rm c57.wav
	${MPLAYER} $@
${D}/c65.jpg: c65
	bash src2jpg c65 $@
${D}/c65.mp3: lso_collingwood/audio_65.wav
	sox lso_collingwood/audio_65.wav c65.wav trim 2.0
	${NORMALISE} c65.wav
	${LAME} -h c65.wav $@
	rm c65.wav
	${MPLAYER} $@
