	${LAME} -h c76.wav $@
	rm c76.wav
	${MPLAYER} $@
${D}/c81.jpg: c81
	bash src2jpg c81 $@
${D}/c81.mp3: lso_collingwood/audio_81.wav
	sox lso_collingwood/audio_81.wav c81.wav trim 2.0
	${NORMALISE} c81.wav
	${LAME} -h c81.wav $@
	rm c81.wav
	${MPLAYER} $@
